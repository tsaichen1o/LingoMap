fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:onClass fibo-fnd-acc-cur:Currency ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "cross-currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "interest rate swap in which the two streams of interest payments are in different currencies" ;
	.

fibo-der-rtd-irswp:FixedFixedInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:exchanges ;
			owl:onClass fibo-der-rtd-irswp:FixedInterestRateLeg ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "fixed fixed interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "interest rate swap in which both parties pay a fixed interest rate that they could not otherwise obtain outside of a swap arrangement" ;
	skos:example "For example, each counterparty uses a different native currency, but wants to borrow money in the other counterparty's native currency." ;
	skos:note "Fixed-fixed swaps generally take the form of either a zero coupon swap or a cross-currency swap." ;
	cmns-av:adaptedFrom "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Fourth edition, 2019-10." ;
	cmns-av:synonym "fixed-fixed interest rate swap" ;
	.

fibo-der-rtd-irswp:FixedFloatCrossCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap ,
		fibo-der-rtd-irswp:FixedFloatInterestRateSwap
		;
	rdfs:label "fixed float cross-currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap
			fibo-der-rtd-irswp:FixedFloatInterestRateSwap
		) ;
	] ;
	skos:definition "interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments and the two streams of interest payments are in different currencies" ;
	cmns-av:synonym
		"fixed float cross currency interest rate swap" ,
		"fixed-float cross-currency interest rate swap"
		;
	.

fibo-der-rtd-irswp:FixedFloatInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Class ;
			owl:intersectionOf (
				[
					a owl:Restriction ;
					owl:onProperty fibo-fnd-rel-rel:exchanges ;
					owl:someValuesFrom fibo-der-rtd-irswp:FixedInterestRateLeg ;
				]
				[
					a owl:Restriction ;
					owl:onProperty fibo-fnd-rel-rel:exchanges ;
					owl:someValuesFrom fibo-der-rtd-irswp:FloatingInterestRateLeg ;
				]
			) ;
		]
		;
	rdfs:label "fixed float interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwap
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:someValuesFrom fibo-der-rtd-irswp:FixedInterestRateLeg ;
			]
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:someValuesFrom fibo-der-rtd-irswp:FloatingInterestRateLeg ;
			]
		) ;
	] ;
	skos:definition "interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments" ;
	cmns-av:synonym "fixed-float interest rate swap" ;
	.

fibo-der-rtd-irswp:FixedFloatSingleCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FixedFloatInterestRateSwap ;
	rdfs:label "fixed float single currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:FixedFloatInterestRateSwap
			fibo-der-rtd-irswp:SingleCurrencyInterestRateSwap
		) ;
	] ;
	skos:definition "interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments and where both payment streams are expressed in terms of the same currency" ;
	cmns-av:synonym "fixed-float single-currency interest rate swap" ;
	.

fibo-der-rtd-irswp:FixedInterestRateLeg
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:FixedLeg ,
		fibo-der-rtd-irswp:InterestRateSwapLeg
		;
	rdfs:label "fixed interest rate leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:disjointWith fibo-der-rtd-irswp:FloatingInterestRateLeg ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwapLeg
			[
				a owl:Restriction ;
				owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
				owl:someValuesFrom fibo-fbc-dae-dbt:FixedInterestRate ;
			]
		) ;
	] ;
	skos:definition "fixed leg that specifies fixed interest amounts and terms for the payment of that interest" ;
	cmns-av:explanatoryNote "This may be the funding leg of some swaps (i.e. one party agrees to pay fixed interest amounts in exchange for whatever is the other leg) or it may be one or both sides of an interest rate swap, where the two parties exchange different interest payment streams." ;
	cmns-av:synonym "fixed interest rate payment stream" ;
	.

fibo-der-rtd-irswp:FloatFloatCrossCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FloatFloatInterestRateSwap ;
	rdfs:label "float float cross-currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap
			fibo-der-rtd-irswp:FloatFloatInterestRateSwap
		) ;
	] ;
	skos:definition "interest rate swap that exchanges cashflows based on two different interest rates in different currencies" ;
	cmns-av:synonym "float-float cross-currency interest rate swap" ;
	.

fibo-der-rtd-irswp:FloatFloatInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:BasisSwap ,
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:exchanges ;
			owl:onClass fibo-der-rtd-irswp:FloatingInterestRateLeg ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "float float interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwap
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:onClass fibo-der-rtd-irswp:FloatingInterestRateLeg ;
				owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
			]
		) ;
	] ;
	skos:definition "interest rate swap that exchanges cashflows based on two different floating interest rates" ;
	cmns-av:adaptedFrom "http://www.investopedia.com/terms/b/basisrateswap.asp"^^xsd:anyURI ;
	cmns-av:explanatoryNote "This is a swap in which two parties swap variable interest rates based on different money markets, and this is usually done to limit interest-rate risk that a company faces as a result of having differing lending and borrowing rates." ;
	cmns-av:synonym
		"basis rate swap" ,
		"float-float interest rate swap"
		;
	.

fibo-der-rtd-irswp:FloatFloatSingleCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FloatFloatInterestRateSwap ;
	rdfs:label "float float single currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:FloatFloatInterestRateSwap
			fibo-der-rtd-irswp:SingleCurrencyInterestRateSwap
		) ;
	] ;
	skos:definition "interest rate swap that exchanges cashflows based on two different floating interest rates in the same currency" ;
	cmns-av:synonym "float-float single-currency interest rate swap" ;
	.

fibo-der-rtd-irswp:FloatingInterestRateLeg
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:FloatingLeg ,
		fibo-der-rtd-irswp:InterestRateSwapLeg ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:onClass fibo-fbc-dae-dbt:InterestCalculationSchedule ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:onClass fibo-fbc-dae-dbt:InterestRateReset ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:onClass fibo-fbc-dae-dbt:InterestRateResetSchedule ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:onClass fibo-fbc-dae-dbt:InterestRateSettingEvent ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "floating interest rate leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwapLeg
			[
				a owl:Restriction ;
				owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
				owl:someValuesFrom fibo-fbc-dae-dbt:FloatingInterestRate ;
			]
		) ;
	] ;
	skos:definition "floating leg in which variable interest is paid on some notional amount, linked to some underlying interest reference rate" ;
	cmns-av:explanatoryNote "Instead of an absolute rate you have either a variable reference rate or fixed reference rate and an offset that varies in some way, called a spread (same as margin in floating rate notes)." ;
	cmns-av:synonym "floating interest rate swap stream" ;
	.

fibo-der-rtd-irswp:InflationLeg
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:FloatingLeg ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fi-fi:hasUnderlier ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty cmns-rlcmp:isPlayedBy ;
				owl:someValuesFrom fibo-ind-ei-ei:EconomicIndicator ;
			] ;
		]
		;
	rdfs:label "inflation leg"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "floating rate leg of an inflation swap linked to an inflation index, such as the Consumer Price Index (CPI)"@en ;
	.

fibo-der-rtd-irswp:InflationSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-bsc:RateBasedDerivative ,
		fibo-der-drc-swp:RatesSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-bnd:isLinkedToFallback ;
			owl:onClass fibo-sec-dbt-bnd:GovernmentBond ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:hasLeg ;
			owl:someValuesFrom fibo-der-rtd-irswp:InflationLeg ;
		]
		;
	rdfs:label "inflation swap"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "rate swap in which one party pays an amount calculated using an inflation rate index, and the other party pays an amount calculated using another inflation rate index, or a fixed or floating interest rate"@en ;
	cmns-av:adaptedFrom "ISO 10962, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fifth edition, 2021-06-15"@en ;
	.

fibo-der-rtd-irswp:InterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-bsc:InterestRateDerivative ,
		fibo-der-drc-swp:RatesSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:exchanges ;
			owl:onClass fibo-der-rtd-irswp:InterestRateSwapLeg ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-drc-swp:Swap
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:onClass fibo-der-rtd-irswp:InterestRateSwapLeg ;
				owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
			]
		) ;
	] ;
	skos:definition "swap in which the reference (underlier) for at least one leg is an interest rate" ;
	.

fibo-der-rtd-irswp:InterestRateSwapLeg
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:RateBasedLeg ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:onClass fibo-der-rtd-irswp:NotionalStepSchedule ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-bd:hasBusinessRecurrenceIntervalConvention ;
			owl:onClass fibo-fbc-dae-dbt:DayCountConvention ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:onClass fibo-fbc-dae-dbt:InterestCalculationSchedule ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pas-psch:hasPaymentSchedule ;
			owl:onClass fibo-fbc-dae-dbt:InterestPaymentSchedule ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:onClass fibo-fnd-acc-cur:Currency ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "interest rate swap leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-drc-swp:RateBasedLeg
			[
				a owl:Restriction ;
				owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
				owl:someValuesFrom fibo-fnd-acc-cur:InterestRate ;
			]
		) ;
	] ;
	skos:definition "swap leg that has an interest rate payment stream, including both a parametric and cashflow representation for the stream of payments" ;
	cmns-av:synonym "interest rate swap stream" ;
	.

fibo-der-rtd-irswp:NotionalStepAmount
	a owl:Class ;
	rdfs:subClassOf
		cmns-qtu:ScalarQuantityValue ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasNotionalAmount ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-fnd-acc-cur:MonetaryAmount
					fibo-fnd-acc-cur:PercentageMonetaryAmount
				) ;
			] ;
		]
		;
	rdfs:label "notional step amount" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "the amount of money that is subtracted from the notional on each step date" ;
	cmns-av:explanatoryNote "Note that this is an actual concrete sum of money, which may be specified either as a monetary amount (e.g. dollars and cents) or as a percentage of either the original notional amount or the previous notional amount." ;
	.

fibo-der-rtd-irswp:NotionalStepChangeEvent
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-pas-fpas:ContractLifecycleEventOccurrence ,
		fibo-sec-dbt-dbti:StepEvent ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:NotionalStepAmount ;
		]
		;
	rdfs:label "notional step change event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "event in which a step change in the notional amount for a given swap leg occurs" ;
	cmns-av:explanatoryNote "The frequency / period length of the steps in the step schedule is a multiple of the calculation period or frequency. For example, if the notional is recalculated on every calculation date, applying a new interest rate to the new notional amount, then the two frequencies are the same. If notional is updated every second calculation period, then the step schedule specifies periods that are twice as long, and so on." ;
	.

fibo-der-rtd-irswp:NotionalStepPeriodLength
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:RecurrenceInterval ;
	rdfs:label "notional step period length" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "recurrence interval indicating the frequency with which step changes occur, which is a multiple of the calculation period in the calculation schedule" ;
	.

fibo-der-rtd-irswp:NotionalStepSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-dae-dbt:ProjectedContractEventSchedule ,
		fibo-sec-dbt-dbti:StepSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasRecurrenceInterval ;
			owl:someValuesFrom fibo-der-rtd-irswp:NotionalStepPeriodLength ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-oc:hasOccurrence ;
			owl:someValuesFrom fibo-der-rtd-irswp:NotionalStepChangeEvent ;
		]
		;
	rdfs:label "notional step schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "schedule of changes in the notional amount on which interest is paid, comprising the regular sequence of step events" ;
	.

fibo-der-rtd-irswp:OvernightIndexSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:hasLeg ;
			owl:someValuesFrom fibo-der-rtd-irswp:OvernightRateIndexLeg ;
		]
		;
	rdfs:label "overnight index swap"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "swap in which the periodic payments for one leg are based on an overnight interest rate index multiplied by the same notional amount upon which payments for the other leg of the swap are based"@en ;
	cmns-av:abbreviation "OIS swap"@en ;
	cmns-av:adaptedFrom "ISO 10962, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fifth edition, 2021-06-15"@en ;
	cmns-av:explanatoryNote "The interest of the overnight rate portion of the swap is compounded and paid at reset dates. The present value for the leg is determined by either compounding of the overnight rate or by taking the geometric average of the rate over a given period."@en ;
	.

fibo-der-rtd-irswp:OvernightRateIndexLeg
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FloatingInterestRateLeg ;
	rdfs:label "overnight rate index leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Restriction ;
		owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
		owl:someValuesFrom fibo-ind-ir-ir:OvernightRate ;
	] ;
	skos:definition "floating leg in which periodic payments are based on an overnight interest rate index multiplied by the same notional amount on which the payments for the other leg of the swap are based" ;
	cmns-av:adaptedFrom "ISO 10962, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fifth edition, 2021-06-15"@en ;
	.

fibo-der-rtd-irswp:PlainVanillaInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:FixedFloatSingleCurrencyInterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasNotionalAmount ;
			owl:onClass fibo-fnd-acc-cur:MonetaryAmount ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:hasLeg ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty cmns-doc:specifies ;
				owl:onClass fibo-der-rtd-irswp:NotionalStepChangeEvent ;
				owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:hasLeg ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
				owl:onClass fibo-der-rtd-irswp:NotionalStepSchedule ;
				owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			] ;
		]
		;
	rdfs:label "plain vanilla interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "fixed-float single currency interest rate swap in which interest payments are netted, the notional principal does not change, and there are no embedded options" ;
	.

fibo-der-rtd-irswp:SingleCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-doc:specifies ;
			owl:onClass fibo-fnd-acc-cur:Currency ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "single currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "interest rate swap in which the two streams of interest payments are in the same currency" ;
	.

fibo-der-rtd-irswp:ZeroCouponInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasNotionalAmount ;
			owl:onClass fibo-fnd-acc-cur:MonetaryAmount ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "zero coupon interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "interest rate swap in which the fixed rate cash flows are compounded and paid once on the expiration date, rather than periodically; the payments on the other side (which can be based on a floating interest rate or a fixed rate) follow typical swap payment schedules" ;
	cmns-av:adaptedFrom "ISO 10962, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fifth edition, 2021-06-15"@en ;
	.

fibo-der-rtd-irswp:hasFirstNotionalStepDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf cmns-dt:hasStartDate ;
	rdfs:label "has first notional step date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	rdfs:range cmns-dt:ExplicitDate ;
	skos:definition "indicates the initial date in a notional step schedule" ;
	.

fibo-der-rtd-irswp:hasFloatingRateCap
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fbc-dae-dbt:hasInterestRate ;
	rdfs:label "has floating rate cap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	rdfs:range fibo-fbc-dae-dbt:FloatingInterestRate ;
	skos:definition "indicates an optional ceiling (cap) on interest rates on floating rate debts" ;
	cmns-av:explanatoryNote "Rate caps can be viewed as insurance, ensuring that the maximum borrowing rate never exceeds the specified cap level." ;
	.

fibo-der-rtd-irswp:hasFloatingRateFloor
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fbc-dae-dbt:hasInterestRate ;
	rdfs:label "has floating rate floor" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	rdfs:range fibo-fbc-dae-dbt:FloatingInterestRate ;
	skos:definition "indicates an optional lower bound on interest rates on floating rate debts" ;
	.

fibo-der-rtd-irswp:hasFloatingRateSpread
	a owl:DatatypeProperty ;
	rdfs:label "has floating rate spread" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	rdfs:range xsd:decimal ;
	skos:definition "defines the spread rate that can optionally be used to adjust the floating rate" ;
	cmns-av:explanatoryNote "Such adjustments may be added to or subtracted from the floating rate." ;
	.

fibo-der-rtd-irswp:hasLastNotionalStepDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf cmns-dt:hasEndDate ;
	rdfs:label "has last notional step date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	rdfs:range cmns-dt:ExplicitDate ;
	skos:definition "indicates the final date in a notional step schedule" ;
	.

fibo-der-rtd-irswp:hasRateMultiplier
	a owl:DatatypeProperty ;
	rdfs:label "has rate multiplier" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	rdfs:range xsd:decimal ;
	skos:definition "indicates a multiplier applied to the coupon before adding the floating rate spread" ;
	.

