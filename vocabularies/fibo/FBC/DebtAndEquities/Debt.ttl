			owl:someValuesFrom fibo-fbc-dae-dbt:InterestPayment ;
		]
		;
	rdfs:label "interest payment schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/DebtAndEquities/Debt/> ;
	skos:definition "regular, contract-specific schedule including the dates on which interest is due to be paid" ;
	cmns-av:explanatoryNote "The dates may be fixed, or expressed as an offset of the calculation dates. Typically the payment dates are fixed and calculation dates are expressed as an offset, however." ;
	.

fibo-fbc-dae-dbt:InterestPaymentTerms
	a owl:Class ;
